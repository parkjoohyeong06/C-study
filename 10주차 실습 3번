#include <stdio.h>
#include <stdlib.h>

int main(void) {
    int n;
    int order;
    int *arr;

    scanf("%d", &n);                     // 몇 개?
    arr = (int *)malloc(sizeof(int)*n);  // 그만큼 동적 할당
    if (arr == NULL) return -1;

    for (int i = 0; i < n; i++)
        scanf("%d", &arr[i]);            // n개 입력

    scanf("%d", &order);                 // 1 오름 / 2 내림

    // 버블 정렬
    for (int i = 0; i < n - 1; i++) {
        for (int j = 0; j < n - 1 - i; j++) {
            int swap = 0;
            if (order == 1 && arr[j] > arr[j+1])      // 오름
                swap = 1;
            else if (order == 2 && arr[j] < arr[j+1]) // 내림
                swap = 1;

            if (swap) {
                int tmp = arr[j];
                arr[j] = arr[j+1];
                arr[j+1] = tmp;
            }
        }
    }

    for (int i = 0; i < n; i++)
        printf("%d ", arr[i]);

    free(arr);
    return 0;
}
